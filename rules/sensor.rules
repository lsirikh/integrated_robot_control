# Set the udev rule to make the device port be fixed by imu and rplidar

# For RPLIDAR (assumed to be ttyUSB1)
KERNEL=="ttyUSB1", ATTRS{idVendor}=="10c4", ATTRS{idProduct}=="ea60", MODE:="0666", SYMLINK+="rplidar"

# For IMU (assumed to be ttyUSB0)
KERNEL=="ttyUSB0", ATTRS{idVendor}=="10c4", ATTRS{idProduct}=="ea60", MODE:="0666", SYMLINK+="imu_usb"

